{
	"name": "codcut",
	"displayName": "Codcut",
	"description": "Share pieces of your code to Codcut.",
	"version": "0.1.4",
	"publisher": "codcut",
	"license": "MIT",
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/codcut/codcut-vscode.git"
	},
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.codcut-share"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Codcut configuration options",
			"properties": {
				"codcut.token": {
					"type": [
						"string"
					],
					"default": "",
					"description": "Specifies the access token directly linked to your account"
				}
			}
		},
		"commands": [
			{
				"command": "extension.codcut-share",
				"title": "Share to Codcut"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "extension.codcut-share",
					"group": "codcut"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/crypto-js": "^3.1.43",
		"@types/express": "^4.16.1",
		"@types/jwt-decode": "^2.2.1",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/node-fetch": "^2.3.4",
		"@types/query-string": "^6.3.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"node-fetch": "^2.6.1"
	}
}
